[cmdbuild]
# Target hostname or IP address
# インストール先ホスト名またはIPアドレス
cmdbuild_hostname

[cmdbuild:vars]
# 基本設定
locale=ja_JP.UTF-8
zone=Asia/Tokyo
zoneinfo_path=/usr/share/zoneinfo/Asia/Tokyo

# Java設定
JAVA_DL_URL=http://download.oracle.com/otn-pub/java/jdk/8u77-b03/jdk-8u77-linux-x64.rpm
JAVA_DL_PATH=/usr/local/src/jdk-8u77-linux-x64.rpm

# TomcatダウンロードURL
TOMCAT_DL_URL=http://ftp.riken.jp/net/apache/tomcat/tomcat-8/v8.0.33/bin/apache-tomcat-8.0.33.tar.gz
TOMCAT_VER=8.0.33

# PostgresリポジトリダウンロードURL
postgre_repo_v6=http://yum.postgresql.org/9.4/redhat/rhel-6-x86_64/pgdg-centos94-9.4-2.noarch.rpm
postgre_repo_v7=http://yum.postgresql.org/9.4/redhat/rhel-7-x86_64/pgdg-centos94-9.4-2.noarch.rpm
JDBC_URL=https://jdbc.postgresql.org/download/postgresql-9.4.1208.jar

# CMDBuild設定
CMDBUILD_VER=2.4.0
CMDBUILD_URL=https://sourceforge.net/projects/cmdbuild/files/2.4.0/cmdbuild-2.4.0.zip
SHARK_URL=https://sourceforge.net/projects/cmdbuild/files/2.4.0/shark-cmdbuild-2.4.0.zip
CONNECTOR_URL=https://sourceforge.net/projects/cmdbuild/files/2.4.0/basic-connector-1.5.0.zip
REPORT_URL=https://sourceforge.net/projects/cmdbuild/files/2.4.0/additional-report-libs-1.5.zip
PORTLET_URL=https://sourceforge.net/projects/cmdbuild/files/2.4.0/cmdbuild-portlet-1.6.1.war